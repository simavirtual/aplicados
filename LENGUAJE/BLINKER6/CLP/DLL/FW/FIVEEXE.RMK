MakePath[.ch]   = "C:\FW\INCLUDE";$(include);

.prg.obj:
  SET INCLUDE=$(MakePath[.ch])
  SET CLIPPERCMD=-m -w -n -q -es2
  clipper $< -o$@

.obj.exe:
  blinker @FIVEEXE.LNK

// Modules
// =========
FiveExe.obj     : FiveExe.prg

FiveExe.exe     : FiveExe.obj

