FI \flipper\fonts\RMN8_15
SEARCH \FLIP50\SUMMER87
BEGINAREA
   mod FILECLIP,HP_CLIP1,PS_CLIP,GRAPHICS,PIE_CLIP,PRT_CLIP FROM SUMMER87
   ALLOCATE \FLIP50\FLIP5
ENDAREA
mod FILL_FIG,LO_OVAL,LOPATARC,LO_OVARC,GETEVENT,SAVEBACK,EGA_HERC FROM FLIP5
mod FIND_PEL,PUT_PEL,FASTLINE,GET_PEL,CALLBIOS,_GFXMOVE,PIC_ASM FROM FLIP5
mod VIDBLOCK,SETEVHND,SETKYBD,FINDB,FINDW,_IDIV,SCREEN,LO_QSORT FROM FLIP5
mod WRTCHAR,_LO_OVAL FROM FLIP5
mod CAD_OHOT,CAD_MOUS,CAD_DRAW,COMM,MSSEARCH,MSONLY FROM FLIP5
LIB \CLIP87\CLIPPER
LIB \CLIP87\EXTEND
