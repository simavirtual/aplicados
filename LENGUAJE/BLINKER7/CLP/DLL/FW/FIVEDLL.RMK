MakePath[.ch]   = "C:\FW\INCLUDE";$(include);

.prg.obj:
  SET INCLUDE=$(MakePath[.ch])
  SET CLIPPERCMD=-m -w -n -q -es2
  clipper $< -o$@

.obj.dll:
  Blinker @FIVEDLL.LNK
  BliLib  /NP /OL FIVEDLL.LIB /ID FIVEDLL.DLL

// Modules
// =========
FIVEDLL.OBJ     : FIVEDLL.PRG

FIVEDLL.DLL     : FIVEDLL.OBJ

